<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="permiss" extends="pts" namespace="/permiss">
		<action name="permiss_*" method="{1}" class="perssAction">
			<result name="success">/pages/permissionSetup.jsp</result>
			<result name="history">/pages/historyRecord.jsp</result>
			<result name="get">/pages/getPermission.jsp</result>
			<result name="ok" type="redirect">permiss/permiss_access.action</result>
		</action>
	</package>
</struts>